{
  "name": "pixel2html-<%= clientId %>-<%= projectId %>",
  "description": "pixel2html <%= clientId %>/<%= projectId %>",
  "license": "MIT",
  "repository": "http://code.pixel2html.com",
  "version": "1.0.3",
  "scripts": {

    "start": "npm install && bower install <% if (markupIntegration=='jekyll'){%>&& bundle install<%}%>",
    "code": "gulp serve",
    "build": "gulp build"
  },
  "dependencies": {
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "browser-sync": "2.6.5",

    <% if (cssProcessor === 'scss' || frontEndFramework) { -%>
    "gulp-sass": "^2.0.4",
    <% } -%>
    <% if (cssProcessor === 'less') { -%>
    "gulp-less": "^3.0.3",
    <% } -%>
    <% if (cssProcessor === 'styl') { -%>
    "gulp-stylus": "^2.0.6",
    <% } -%>

    <% if (markupLanguage === 'pug') { %>
      "gulp-pug": "^3.1.0",
    <% } %>

    "gulp-autoprefixer": "^2.3.1",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-jshint": "^1.12.0",

    "gulp-sourcemaps": "^1.6.0",

    "gulp-util": "^3.0.7",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",

    "require-dir": "^0.3.0",
    "gulp-zip": "^3.2.0"


  }
}
